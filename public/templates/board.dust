{>"layouts/master" /}

{<body}
    <h1>
        Viewing: <a href="{board.shortUrl}">{board.name}</a>
    </h1>
    <h2>
        We have some lists:
    </h2>
    <ul>
        {#lists}
            <li>{name}</li>
        {/lists}
    </ul>
    <h2>And we have some cards</h2>
    <ul>
        {#cards}
            <li>{name} {#labels}{color}{/labels}</li>
        {/cards}
    </ul>
{/body}

